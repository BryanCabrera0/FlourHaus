// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model MenuItem{
  id          Int     @id @default(autoincrement())
  name        String
  description String
  price       Float
  category    String
}
